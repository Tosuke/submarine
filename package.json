{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prestart": "cp -r App.app.tsx App.tsx",
    "start": "expo start",
    "prestorybook": "rnstl && cp -r App.storybook.tsx App.tsx",
    "storybook": "expo start",
    "format": "prettier --write \"{src,storybook}/**/*.{js,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "generate:app": "ts-node tools/generate-appjson.ts",
    "generate:preview-page": "ts-node tools/generate-preview-page.ts",
    "export": "cp -r App.app.tsx App.tsx && yarn generate:app && expo export",
    "publish": "cp -r App.app.tsx App.tsx && expo publish",
    "eject": "expo eject"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@expo/vector-icons": "^10.0.0",
    "@linkage-community/bottlemail": "^2.1.0",
    "@use-it/interval": "^0.1.3",
    "color": "^3.1.2",
    "core-js": "^3.5.0",
    "date-fns": "^2.8.1",
    "expo": "^36.0.1",
    "expo-av": "~8.0.0",
    "expo-constants": "~8.0.0",
    "expo-web-browser": "~8.0.0",
    "hoist-non-react-statics": "^3.3.1",
    "ky": "^0.16.1",
    "pictograph": "^2.3.0",
    "punycode": "^2.1.1",
    "react": "16.9.0",
    "react-dom": "16.12.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-paper": "^2.16.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-status-bar-height": "^2.4.0",
    "react-native-web": "^0.11.7",
    "react-navigation": "3.13.0",
    "rxjs": "^6.5.3",
    "styled-components": "^4.4.1",
    "transform-ts": "^0.2.2"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-ondevice-knobs": "^5.2.8",
    "@storybook/addon-ondevice-notes": "^5.2.8",
    "@storybook/react-native": "^5.2.8",
    "@types/color": "^3.0.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/react": "^16.9.16",
    "@types/react-native": "^0.60.25",
    "@types/styled-components": "^4.4.1",
    "babel-preset-expo": "^8.0.0",
    "emotion-theming": "^10.0.19",
    "handlebars": "^4.5.3",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-native-storybook-loader": "^1.8.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "private": true
}
