{
  "scripts": {
    "prestart": "cp -r App.app.tsx App.tsx",
    "start": "react-native start",
    "prestorybook": "rnstl && cp -r App.storybook.tsx App.tsx",
    "storybook": "react-native start",
    "format": "prettier --write \"{src,storybook}/**/*.{js,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "generate:preview-page": "ts-node tools/generate-preview-page.ts",
    "export": "cp -r App.app.tsx App.tsx && expo export",
    "publish": "cp -r App.app.tsx App.tsx && expo publish",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "postinstall": "jetify"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@linkage-community/bottlemail": "^2.1.0",
    "@react-native-community/masked-view": "0.1.5",
    "@react-navigation/bottom-tabs": "^5.0.0-alpha.34",
    "@react-navigation/native": "^5.0.0-alpha.26",
    "@react-navigation/native-stack": "^5.0.0-alpha.24",
    "@react-navigation/stack": "^5.0.0-alpha.58",
    "@use-it/interval": "^0.1.3",
    "color": "^3.1.2",
    "core-js": "^3.6.4",
    "date-fns": "^2.9.0",
    "expo": "^36.0.2",
    "expo-av": "~8.0.0",
    "expo-web-browser": "~8.0.0",
    "hoist-non-react-statics": "^3.3.1",
    "ky": "^0.16.1",
    "pictograph": "^2.3.0",
    "punycode": "^2.1.1",
    "react": "~16.9.0",
    "react-dom": "16.12.0",
    "react-native": "^0.61.5",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-paper": "^3.4.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.4.0",
    "react-native-unimodules": "~0.7.0",
    "react-native-web": "^0.11.7",
    "rxjs": "^6.5.4",
    "styled-components": "^4.4.1",
    "transform-ts": "^0.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@react-native-community/cli": "^3.0.4",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-ondevice-knobs": "^5.2.8",
    "@storybook/addon-ondevice-notes": "^5.3.3",
    "@storybook/react-native": "^5.3.3",
    "@types/color": "^3.0.1",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/react": "^16.9.17",
    "@types/react-native": "^0.60.30",
    "@types/styled-components": "^4.4.2",
    "babel-jest": "~24.9.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-expo": "^8.0.0",
    "emotion-theming": "^10.0.27",
    "handlebars": "^4.7.2",
    "husky": "^4.0.10",
    "jest": "~24.9.0",
    "jetifier": "~1.6.4",
    "lint-staged": "^9.5.0",
    "metro-react-native-babel-preset": "~0.56.0",
    "prettier": "^1.19.1",
    "react-native-storybook-loader": "^1.8.1",
    "react-test-renderer": "~16.12.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.4"
  },
  "private": true
}
