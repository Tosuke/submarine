{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prestart": "cp -r App.app.tsx App.tsx",
    "start": "expo start",
    "prestorybook": "cp -r App.storybook.tsx App.tsx",
    "storybook": "expo start",
    "publish-expo": "cp -r App.app.tsx App.tsx && expo publish",
    "eject": "expo eject",
    "format": "prettier --write \"{src,storybook}/**/*.{js,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "generate:app": "ts-node tools/generate-appjson.ts",
    "generate:preview-page": "ts-node tools/generate-preview-page.ts",
    "export": "cp -r App.app.tsx App.tsx && yarn generate:app && expo export",
    "lint-staged": "lint-staged",
    "pre-commit": "npm run lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --check"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@linkage-community/bottlemail": "^0.1.5",
    "color": "^3.1.2",
    "core-js": "^3.1.4",
    "date-fns": "^1.30.1",
    "expo": "^35.0.0",
    "expo-av": "^7.0.1",
    "expo-constants": "^7.0.0",
    "expo-web-browser": "~7.0.0",
    "hoist-non-react-statics": "^3.3.0",
    "ky": "^0.15.0",
    "pictograph": "^2.2.0",
    "punycode": "^2.1.1",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-paper": "^2.16.0",
    "react-native-reanimated": "~1.2.0",
    "react-native-screens": "~1.0.0-alpha.23",
    "react-native-status-bar-height": "^2.4.0",
    "react-native-web": "^0.11.7",
    "react-navigation": "^3.11.0",
    "rxjs": "^6.5.2",
    "transform-ts": "^0.2.2"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-ondevice-knobs": "^5.1.11",
    "@storybook/addon-ondevice-notes": "^5.1.11",
    "@storybook/react-native": "^5.1.11",
    "@types/color": "^3.0.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.57.65",
    "babel-preset-expo": "^7.0.0",
    "emotion-theming": "^10.0.14",
    "handlebars": "^4.5.2",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.4.5"
  },
  "private": true
}
