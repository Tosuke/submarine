{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prestart": "cp -r App.app.tsx App.tsx",
    "start": "expo start",
    "prestorybook": "cp -r App.storybook.tsx App.tsx",
    "storybook": "expo start",
    "publish-expo": "cp -r App.app.tsx App.tsx && expo publish",
    "eject": "expo eject",
    "format": "prettier --write \"{src,storybook}/**/*.{js,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "pre-commit": "npm run type-check && npm run lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --check"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.3",
    "color": "^3.1.2",
    "core-js": "^3.1.4",
    "date-fns": "^1.30.1",
    "expo": "^34.0.3",
    "expo-av": "~6.0.0",
    "expo-constants": "~6.0.0",
    "hoist-non-react-statics": "^3.3.0",
    "ky": "^0.11.1",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-paper": "^2.16.0",
    "react-native-reanimated": "~1.1.0",
    "react-native-screens": "1.0.0-alpha.22",
    "react-native-web": "^0.11.4",
    "react-navigation": "^3.11.0",
    "rxjs": "^6.5.2",
    "transform-ts": "^0.2.2"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-ondevice-knobs": "^5.1.11",
    "@storybook/addon-ondevice-notes": "^5.1.11",
    "@storybook/react-native": "^5.1.11",
    "@types/color": "^3.0.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "babel-preset-expo": "^5.1.1",
    "emotion-theming": "^10.0.14",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "typescript": "^3.4.5"
  },
  "private": true
}
